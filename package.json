{
	"name": "ts-prisma-case",
	"version": "1.0.0",
	"description": "A minimalistic TypeScript starter repository for building applications with Prisma and PostgreSQL.",
	"main": "index.js",
	"scripts": {
		"start": "npx tsx watch src/App.ts",
		"build": "npx tsup src --format cjs --dts --minify --clean --sourcemap",
