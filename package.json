		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"keywords": [
		"Node",
		"TypeScript",
		"Prisma",
		"Docker"
	],
	"author": "Yan Avlis",
	"license": "MIT",
	"devDependencies": {
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/node": "20.11.17",
		"prettier": "3.2.5",
		"prisma": "5.9.1",
		"tsup": "8.0.1",
		"tsx": "4.7.0",
		"typescript": "5.3.3"
	},
	"dependencies": {
		"@prisma/client": "5.9.1",
		"cors": "^2.8.5",
		"express": "^4.18.2"
	}
}
